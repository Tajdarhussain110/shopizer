pipeline {
    agent {label 'GRADLE && JDK11 && PY3 && NVM'} 
    triggers {
        pollSCM('* * * * *')
    }
    stages {
        stage('clone') { 
            steps {
                git url:'https://github.com/Tajdarhussain110/shopizer.git' , branch : 'SPRINT_1_REL' 
            }
        }
        stage('install') { 
            steps {
                sh 'mvn package'
            }
        }
    }
}